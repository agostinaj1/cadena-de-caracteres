#include <stdio.h>
#include <string.h>

int main() {
    char palabra1[50];
    char palabra2[50];

    printf("Ingrese la primera palabra: ");
    scanf("%49s", palabra1);

    printf("Ingrese la segunda palabra: ");
    scanf("%49s", palabra2);

    if (strcmp(palabra1, palabra2) == 0) {
        printf("Las palabras son iguales.\n");
    } else {
        printf("Las palabras son diferentes.\n");
    }

    return 0;
}
